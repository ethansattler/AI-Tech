import java.io.BufferedReader;
import java.io.FileReader;
import java.io.InputStream;
import java.net.URL;
import java.util.ArrayList;
import java.util.List;

class Natural_Parser {
	
	public static List<String> verbs = new ArrayList<String>();
	public URL urlToVerbs = this.getClass().getResource("/" + "VERBS.txt");
	
	public void main (String[] args)  
	{
		set_verb_list();
		//System.out.println("Not the correct way to execute this file, refer to documentation.");
		System.exit(0);
	}
	
	public void set_verb_list() {
		try {
			URL urlToDictionary = this.getClass().getResource("/" + "Dictionary.txt");
			InputStream stream = urlToDictionary.openStream();
			String verb_list = stream.toString();
		String[] split_list = verb_list.split(",");
		int len = split_list.length;
		int x = 0;
		while (x < len) {
			verbs.add(split_list[x]);
			x++;
		}
		} catch (Exception ex) {
			
		}
	}
}